{
  "name": "apps4web",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build && gzipper compress ./dist",
    "serve:build": "lite-server --baseDir=\"dist/apps4web\"\n",
    "build:prod:serve": "ng build --configuration production && lite-server --baseDir=\"dist/apps4web\"\n",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "build:stats": "ng build --stats-json",
    "serve:stats": "webpack-bundle-analyzer ./dist/apps4web/stats.json",
    "ng": "ng"
  },
