{
  "name": "apps4web",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build && gzipper compress ./dist",
    "serve": "http-server -a 127.0.0.1 -p 4300",
    "serve-new": "http-server ./dist/apps4web -a 127.0.0.1 -p 4300",
    "serve:build-dev": "http-server ./dist/apps4web --port 4200",
    "serve:build_lite": "lite-server --baseDir=\"dist/apps4web\"\n",
    "build:prod:serve": "ng build --configuration production && lite-server --baseDir=\"dist/apps4web\"\n",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "build:stats": "ng build --stats-json",
    "serve:stats": "webpack-bundle-analyzer ./dist/apps4web/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.1",
    "@angular/cdk": "^17.1.1",
    "@angular/common": "^17.1.1",
    "@angular/compiler": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@angular/forms": "^17.1.1",
    "@angular/material": "^17.1.1",
    "@angular/material-date-fns-adapter": "^17.1.1",
    "@angular/platform-browser": "^17.1.1",
    "@angular/platform-browser-dynamic": "^17.1.1",
    "@angular/router": "^17.1.1",
    "@angular/service-worker": "^17.1.1",
    "@ngxs/devtools-plugin": "3.8.2",
    "@ngxs/store": "3.8.2",
    "date-fns": "^3.3.1",
    "exifreader": "^4.9.1",
    "file-saver": "^2.0.5",
    "ngx-scrollbar": "^13.0.3",
    "ngx-socket-io": "^4.5.1",
    "rxjs": "^7.8.0",
    "three": "^0.144.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.1.1",
    "@angular/compiler-cli": "^17.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/google-one-tap": "^1.2.2",
    "@types/jasmine": "~4.3.5",
    "@types/node": "^20.5.6",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.47.0",
    "jasmine-core": "~5.1.1",
    "jasmine-marbles": "~0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
