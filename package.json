{
  "name": "apps4web",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build && gzipper compress ./dist",
    "serve:build": "lite-server --baseDir=\"dist/apps4web\"\n",
    "build:prod:serve": "ng build --configuration production && lite-server --baseDir=\"dist/apps4web\"\n",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "build:stats": "ng build --stats-json",
    "serve:stats": "webpack-bundle-analyzer ./dist/apps4web/stats.json",
    "ng": "ng"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "15.0.0",
    "@angular/material-date-fns-adapter": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@ngxs/devtools-plugin": "3.7.5",
    "@ngxs/store": "3.7.5",
    "@types/google-one-tap": "^1.2.2",
    "file-saver": "^2.0.5",
    "ngx-scrollbar": "^10.0.1",
    "rxjs": "^7.5.7",
    "signature_pad": "^4.0.9",
    "three": "^0.144.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "14.3.0",
    "@angular-eslint/eslint-plugin": "14.3.0",
    "@angular-eslint/eslint-plugin-template": "14.3.0",
    "@angular-eslint/schematics": "14.3.0",
    "@angular-eslint/template-parser": "14.3.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^3.8.0",
    "@types/node": "^18.11.9",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "compression": "^1.7.4",
    "eslint": "^8.23.1",
    "lite-server": "^2.6.1",
    "typescript": "^4.8.4",
